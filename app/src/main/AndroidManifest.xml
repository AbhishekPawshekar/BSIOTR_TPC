<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bsiotr_tpc">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bsiotr_TPC"
        android:usesCleartextTraffic="true">
        <activity android:name=".messages.chatting_layout"></activity>
        <activity android:name=".navigationbar_option_layout.contact_basic_edit" />
        <activity android:name=".navigationbar_option_layout.edu_tech_hobbies_edit" />
        <activity android:name=".registration_page2" />
        <activity android:name=".navigationbar_option_layout.profile" />
        <activity android:name=".navigationbar_option_layout.calendar" />
        <activity android:name=".admin.admin_view" />
        <activity android:name=".registration_page" />
        <activity android:name=".practice_test.practice_test" />
        <activity android:name=".placement.placement" />
        <activity android:name=".login__page" />
        <activity android:name=".messages.messages" />
        <activity android:name=".people.people" />
        <activity android:name=".main_page" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>